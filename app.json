{
  "expo": {
    "name": "wallpaperApp",
    "slug": "wallpaperApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/play_store.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.abhishrestha.wallpaperApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "acd47570-65d2-4a59-a7d8-e6193d8b143b"
      }
    }
  }
}
